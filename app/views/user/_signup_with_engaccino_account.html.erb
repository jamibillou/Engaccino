<div class="left dashed">
  <h2><%=t 'user.new.create_engaccino_account' %></h2>
  <%= f.text_field :email, :value => t('user.new.email_field'), :id => 'signup_email_input', :class => 'field' %><br />
  <%= f.text_field :password, :value => t('user.new.password_field'), :id => 'signup_password_input', :class => 'field' %><br />
  <%= f.text_field :password_confirmation, :value => t('user.new.confirmation_field'), :id => 'signup_password_confirmation_input', :class => 'field' %><br />
  <br />
  <%= link_to t('user.new.set_up_profile'), '#', :id => 'signup_form_submit_1', :class => 'button blue round' %>
</div>