<%= render 'import' unless signed_up? %>
<%= form_for @candidate, :html => { :id => 'candidate_edit_form' } do |candidate_builder| %>
  <%= render 'resume_share_view', :candidate => @candidate, :f => candidate_builder if signed_up? %>
  <br />
  <%= render 'personal_information',    :f => candidate_builder %>
  <%= render 'professional_experience', :f => candidate_builder %>
  <%= render 'education',               :f => candidate_builder %>
  <%= render 'language_candidate',		  :f => candidate_builder %>
  <%= render 'status',                  :f => candidate_builder %>
  <%= candidate_builder.submit t('candidates.show.view_profile'), :class => 'center' unless signed_up? %>
<% end %>