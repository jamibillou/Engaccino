<div id="signup">
  <%= render 'signup_speed_up' %>
  <%= form_for @candidate, :html => { :id => 'candidate_signup_form' } do |candidate_builder| %>
    <%= render :partial => 'signup_personal_information',    :locals => { :f => candidate_builder } %>
    <%= render :partial => 'signup_professional_experience', :locals => { :f => candidate_builder } %>
    <%= render :partial => 'signup_education',               :locals => { :f => candidate_builder } %>
    <%= render :partial => 'signup_status',                  :locals => { :f => candidate_builder } %>
    <%= candidate_builder.submit t('candidates.edit.view_profile'), :class => 'center' %>
  <% end %>
</div>