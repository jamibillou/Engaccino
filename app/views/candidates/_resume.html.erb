<div id="resume">
  <%= render 'resume_timeline', :candidate => @candidate %>
  <div class="block" id="experiences">
  	<%= render 'resume_experiences', :candidate => @candidate %>
  </div>
  <div class='block' id='educations'>
  	<%= render 'resume_educations', :candidate => @candidate %>
  </div>
  <div class='small_block'>
    <%= render 'resume_languages', :candidate => @candidate %>
  </div>
  <div class='small_block' style="margin-left: 26%;">
  	<%= render 'resume_certifications', :candidate => @candidate %>
  </div>
  <div class='small_block' style="margin-left: 52%">
    <%= render 'resume_miscellaneous', :candidate => @candidate %>	
  </div>   
</div>